# DevLM Environment Variables

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=devlm_db
DB_USER=your_db_username
DB_PASSWORD=your_db_password
DB_MAX_CONNECTIONS=100
DB_CONNECTION_TIMEOUT=30
DB_IDLE_TIMEOUT=300
DB_SSL_MODE=require
DB_POOL_MAX_CONNS=20
DB_POOL_MIN_CONNS=5
DB_CONNECTION_POOL_SIZE=20

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_POOL_SIZE=20
REDIS_MIN_IDLE_CONNECTIONS=5
REDIS_MAX_RETRIES=3
REDIS_IDLE_TIMEOUT=300
REDIS_SENTINEL_MASTER=mymaster
REDIS_SENTINEL_NODES=sentinel1:26379,sentinel2:26379,sentinel3:26379
REDIS_CLUSTER_NODES=node1:7000,node2:7001,node3:7002
REDIS_SSL_ENABLED=true
REDIS_SSL_CERT_PATH=/path/to/redis/cert.pem

# LLM API Configuration
LLM_API_KEY=your_llm_api_key
LLM_API_URL=https://api.example.com/llm
LLM_API_TIMEOUT=60
LLM_API_MAX_RETRIES=3
LLM_API_PROVIDERS=provider1,provider2,provider3
LLM_API_FALLBACK_STRATEGY=round_robin
LLM_API_RATE_LIMIT=100
LLM_API_CONCURRENT_REQUESTS=10
LLM_API_KEY_ROTATION_INTERVAL=86400
LLM_API_KEY_VAULT_PATH=/path/to/llm/api/keys
LLM_API_FALLBACK_THRESHOLD=3
LLM_API_CIRCUIT_BREAKER_THRESHOLD=5

# Search Engine API Configuration
SEARCH_API_KEY=your_search_api_key
SEARCH_API_URL=https://api.example.com/search
SEARCH_API_TIMEOUT=30
SEARCH_API_MAX_RESULTS=50
SEARCH_API_PROVIDERS=provider1,provider2,provider3
SEARCH_API_CACHE_TTL=3600
SEARCH_API_RATE_LIMIT=100
SEARCH_API_CUSTOM_PARAMS=param1:value1,param2:value2
SEARCH_API_RESULT_FILTER=adult_content:false,language:en
SEARCH_API_CUSTOM_ENGINE_ID=your_custom_engine_id

# Code Execution Engine
SANDBOX_IMAGE=devlm/sandbox:latest
MAX_EXECUTION_TIME=30
MAX_MEMORY=512
CPU_LIMIT=1
NETWORK_ACCESS=false
ALLOWED_SYSCALLS=read,write,open,close
SANDBOX_TIMEOUT=60
SANDBOX_CONCURRENT_EXECUTIONS=5
SANDBOX_RESOURCE_MONITORING_INTERVAL=5
SANDBOX_CUSTOM_IMAGES=image1:tag1,image2:tag2
SANDBOX_OUTPUT_LIMIT=10MB
SANDBOX_DISK_QUOTA=100MB

# API Gateway
API_PORT=8080
API_SECRET_KEY=your_api_secret_key
API_RATE_LIMIT_REQUESTS=100
API_RATE_LIMIT_DURATION=60
API_TIMEOUT=30
API_VERSION=v1
API_DOCS_ENABLED=true
API_CORS_ENABLED=true
API_VERSIONING_STRATEGY=url_path
API_DEPRECATION_HEADER=X-API-Deprecated
API_DEPRECATION_DATE_HEADER=X-API-Deprecated-Date

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=/var/log/devlm.log
LOG_ROTATION_SIZE=100MB
LOG_ROTATION_COUNT=5
LOG_FORMAT=json
LOG_OUTPUT=file,stdout
LOG_SYSLOG_ENABLED=false
LOG_SYSLOG_SERVER=localhost:514
LOG_AGGREGATION_SERVICE=logstash
LOG_AGGREGATION_HOST=logstash.example.com
LOG_AGGREGATION_PORT=5000
LOG_ANONYMIZATION_FIELDS=email,ip_address,credit_card
LOG_TRACING_ENABLED=true

# File System
PROJECT_ROOT=/path/to/project/root
TEMP_DIRECTORY=/tmp/devlm
MAX_FILE_SIZE=10MB
ALLOWED_EXTENSIONS=.py,.js,.txt,.md
FILE_CLEANUP_INTERVAL=3600
FILE_MAX_AGE=86400
FILE_INTEGRITY_CHECK_ENABLED=true
FILE_INTEGRITY_CHECK_INTERVAL=86400
FILE_VERSIONING_ENABLED=true
FILE_BACKUP_ENABLED=true
FILE_BACKUP_INTERVAL=86400
FILE_ENCRYPTION_ENABLED=true
FILE_ENCRYPTION_KEY=your_file_encryption_key

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://devlm.example.com
SSL_CERT_PATH=/path/to/ssl/cert.pem
SSL_KEY_PATH=/path/to/ssl/key.pem
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=3600
JWT_REFRESH_EXPIRATION=604800
IP_WHITELIST=127.0.0.1,192.168.1.0/24
IP_RATE_LIMIT_ENABLED=true
IP_RATE_LIMIT_REQUESTS=1000
IP_RATE_LIMIT_DURATION=3600
TWO_FACTOR_AUTH_ENABLED=true
SECURITY_HEADERS_ENABLED=true
CONTENT_SECURITY_POLICY=default-src 'self'; script-src 'self' 'unsafe-inline'
XSS_PROTECTION_ENABLED=true

# Performance
MAX_CONCURRENT_TASKS=5
CACHE_TTL=3600
DB_CONNECTION_POOL_SIZE=20
WORKER_PROCESSES=4
THREAD_POOL_SIZE=10
TASK_QUEUE_SIZE=1000
ADAPTIVE_CONCURRENCY_ENABLED=true
ADAPTIVE_CONCURRENCY_MIN=1
ADAPTIVE_CONCURRENCY_MAX=20
ADAPTIVE_CONCURRENCY_STEP=1
PERFORMANCE_MONITORING_ENABLED=true
AUTO_SCALING_ENABLED=true
AUTO_SCALING_CPU_THRESHOLD=80
AUTO_SCALING_MEMORY_THRESHOLD=85

# Feature Flags
ENABLE_WEB_SEARCH=true
ENABLE_CODE_EXECUTION=true
ENABLE_FILE_OPERATIONS=true
ENABLE_ADVANCED_ANALYTICS=false
ENABLE_REAL_TIME_COLLABORATION=false
ENABLE_CUSTOM_PLUGINS=false
FEATURE_FLAG_PROVIDER=launchdarkly
FEATURE_FLAG_SDK_KEY=your_launchdarkly_sdk_key
AB_TESTING_ENABLED=true
FEATURE_FLAG_POLLING_INTERVAL=60
FEATURE_FLAG_OFFLINE_MODE=false

# Monitoring
PROMETHEUS_PORT=9090
METRICS_ENDPOINT=/metrics
HEALTH_CHECK_ENDPOINT=/health
CUSTOM_METRICS=metric1,metric2,metric3
TRACING_ENABLED=true
TRACING_SAMPLER_RATE=0.1
ALERTING_ENABLED=true
ALERTING_PROVIDER=pagerduty
ALERTING_API_KEY=your_pagerduty_api_key
ANOMALY_DETECTION_ENABLED=true
CUSTOM_DASHBOARDS_ENABLED=true
MONITORING_RETENTION_PERIOD=30d
MONITORING_SCRAPE_INTERVAL=15s

# LLM Instances
LLM_INSTANCES=[{"name":"instance1","model":"gpt-3.5-turbo","max_tokens":1000,"temperature":0.7},{"name":"instance2","model":"gpt-4","max_tokens":2000,"temperature":0.5}]
LLM_SCALING_MIN_INSTANCES=1
LLM_SCALING_MAX_INSTANCES=10
LLM_SCALING_CPU_THRESHOLD=80
LLM_INSTANCE_HEALTH_CHECK_INTERVAL=60
LLM_LOAD_BALANCING_STRATEGY=least_connections
LLM_FAILOVER_ENABLED=true
LLM_WARM_UP_STRATEGY=gradual
LLM_INSTANCE_STARTUP_TIMEOUT=300

# Backup Configuration
BACKUP_SCHEDULE=0 0 * * *
BACKUP_RETENTION_PERIOD=30
BACKUP_STORAGE_TYPE=s3
BACKUP_S3_BUCKET=devlm-backups
BACKUP_S3_REGION=us-west-2
BACKUP_ENCRYPTION_KEY=your_backup_encryption_key
BACKUP_COMPRESSION_ENABLED=true
BACKUP_INCREMENTAL_ENABLED=true
BACKUP_VERIFICATION_ENABLED=true
BACKUP_RESTORATION_TESTING_ENABLED=true
BACKUP_MULTI_REGION_ENABLED=true
BACKUP_SECONDARY_REGION=eu-west-1

# Email Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USERNAME=your_smtp_username
SMTP_PASSWORD=your_smtp_password
EMAIL_FROM_ADDRESS=noreply@devlm.example.com
EMAIL_TEMPLATE_DIR=/path/to/email/templates
EMAIL_RATE_LIMIT=100
EMAIL_PROVIDER_FALLBACK=sendgrid,mailgun,ses
EMAIL_TRACKING_ENABLED=true
EMAIL_ANALYTICS_ENABLED=true
EMAIL_BOUNCE_HANDLING_ENABLED=true
EMAIL_SPAM_SCORE_THRESHOLD=5

# Sentry Configuration
SENTRY_DSN=https://your-sentry-dsn.ingest.sentry.io/project-id
SENTRY_ENVIRONMENT=production
SENTRY_RELEASE=1.0.0
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_MAX_BREADCRUMBS=100
SENTRY_CUSTOM_ERROR_GROUPING=true
SENTRY_ERROR_FILTER_REGEX=^(Exception|Error):.*
SENTRY_PERFORMANCE_MONITORING_ENABLED=true
SENTRY_IGNORE_ERRORS=RateLimitExceeded,NetworkTimeout
SENTRY_SANITIZE_FIELDS=password,credit_card,ssn

# gRPC Configuration
GRPC_PORT=50051
GRPC_MAX_CONCURRENT_STREAMS=100
GRPC_KEEPALIVE_TIME=60
GRPC_KEEPALIVE_TIMEOUT=20
GRPC_TLS_CERT_PATH=/path/to/grpc/cert.pem
GRPC_TLS_KEY_PATH=/path/to/grpc/key.pem
GRPC_COMPRESSION_ENABLED=true
GRPC_INTERCEPTORS=auth,logging,tracing
GRPC_SERVICE_DISCOVERY_ENABLED=true
GRPC_MAX_MESSAGE_SIZE=4MB
GRPC_REFLECTION_ENABLED=true

# Microservices Configuration
LLM_SERVICE_HOST=localhost
LLM_SERVICE_PORT=5000
ACTION_EXECUTOR_HOST=localhost
ACTION_EXECUTOR_PORT=5001
CODE_EXECUTION_HOST=localhost
CODE_EXECUTION_PORT=5002
SERVICE_DISCOVERY_ENABLED=true
SERVICE_DISCOVERY_PROVIDER=consul
SERVICE_DISCOVERY_CONSUL_HOST=consul.example.com
SERVICE_DISCOVERY_CONSUL_PORT=8500
CIRCUIT_BREAKER_ENABLED=true
RETRY_MECHANISM_ENABLED=true
SERVICE_MESH_ENABLED=true
SERVICE_MESH_PROVIDER=istio

# CORS Configuration
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_HEADERS=Content-Type,Authorization,X-Requested-With
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOW_ORIGINS=http://localhost:3000,https://devlm.example.com
CORS_MAX_AGE=3600
CORS_EXPOSED_HEADERS=X-Custom-Header
CORS_DYNAMIC_ORIGIN_ENABLED=true
CORS_PREFLIGHT_CONTINUE=false

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_SECOND=10
RATE_LIMIT_BURST=20
RATE_LIMIT_STRATEGY=token_bucket
RATE_LIMIT_PER_IP=true
RATE_LIMIT_PER_USER=true
RATE_LIMIT_REDIS_ENABLED=true
RATE_LIMIT_ADAPTIVE_ENABLED=true
RATE_LIMIT_HEADERS_ENABLED=true
RATE_LIMIT_EXCEED_ACTION=block

# Caching Configuration
CACHE_DRIVER=redis
CACHE_PREFIX=devlm
CACHE_DEFAULT_TTL=3600
CACHE_SECOND_LEVEL_ENABLED=true
CACHE_SECOND_LEVEL_DRIVER=memcached
CACHE_WARMING_ENABLED=true
CACHE_INVALIDATION_STRATEGY=time-based
CACHE_COMPRESSION_ENABLED=true
CACHE_COMPRESSION_ALGORITHM=lz4

# Websocket Configuration
WS_ENABLED=true
WS_PORT=8081
WS_PATH=/ws
WS_MAX_CONNECTIONS=1000
WS_PING_INTERVAL=30
WS_PING_TIMEOUT=10
WS_AUTH_ENABLED=true
WS_AUTH_PROVIDER=jwt
WS_COMPRESSION_ENABLED=true
WS_SSL_ENABLED=true

# Internationalization
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,es,fr,de
TRANSLATION_FILE_PATH=/path/to/translations
AUTO_TRANSLATION_ENABLED=true
AUTO_TRANSLATION_PROVIDER=google_translate
REAL_TIME_LANGUAGE_SWITCHING_ENABLED=true
FALLBACK_LANGUAGE=en
TRANSLATION_MEMORY_ENABLED=true

# Pagination
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=100
PAGINATION_STYLE=cursor
ADAPTIVE_PAGINATION_ENABLED=true
PAGINATION_HEADERS_ENABLED=true
TOTAL_COUNT_HEADER=X-Total-Count

# Encryption
ENCRYPTION_KEY=your_encryption_key
ENCRYPTION_ALGORITHM=AES-256-GCM
ENCRYPTION_KEY_ROTATION_INTERVAL=2592000
KEY_MANAGEMENT_SYSTEM_ENABLED=true
KEY_MANAGEMENT_SYSTEM_PROVIDER=aws-kms
DATA_AT_REST_ENCRYPTION_ENABLED=true
ENCRYPTED_FIELDS=password,credit_card,ssn

# External Services
GITHUB_API_TOKEN=your_github_api_token
GITLAB_API_TOKEN=your_gitlab_api_token
BITBUCKET_API_TOKEN=your_bitbucket_api_token
JIRA_API_TOKEN=your_jira_api_token
CONFLUENCE_API_TOKEN=your_confluence_api_token
EXTERNAL_SERVICE_OAUTH2_ENABLED=true
EXTERNAL_SERVICE_RATE_LIMIT_ENABLED=true
EXTERNAL_SERVICE_CACHE_ENABLED=true

# Notification Service
NOTIFICATION_SERVICE_ENABLED=true
NOTIFICATION_SERVICE_PROVIDER=sendgrid
NOTIFICATION_SERVICE_API_KEY=your_notification_service_api_key
NOTIFICATION_FALLBACK_PROVIDER=mailgun
NOTIFICATION_CHANNELS=email,sms,push
NOTIFICATION_TEMPLATES_ENABLED=true
NOTIFICATION_PERSONALIZATION_ENABLED=true
NOTIFICATION_BATCHING_ENABLED=true
NOTIFICATION_DELIVERY_STATUS_TRACKING=true

# Machine Learning
ML_MODEL_PATH=/path/to/ml/models
ML_INFERENCE_TIMEOUT=30
ML_MAX_BATCH_SIZE=32
ML_MODEL_UPDATE_INTERVAL=86